<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="search_box">
        <attr name="search_hint" format="string|reference"/><!--EditText hint-->
        <attr name="search_background" format="reference"/><!--EditText  background-->
        <attr name="search_only_search" format="enum"> <!-- Is it just search -->
            <enum name="yes" value="0"/>
            <enum  name="no" value="1"/>
        </attr>
        <attr name="search_status" format="enum"><!--  -->
            <enum name="visible" value="0"/>
            <enum name="invisible" value="1"/>
            <enum name="gone" value="-1"/>
        </attr>
        <attr name="search_save_old_data_size" format="integer"/><!-- List Save Data Count -->
    </declare-styleable>
</resources>
